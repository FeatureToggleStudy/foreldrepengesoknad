{
  "name": "foreldrepengesoknad",
  "version": "1.0.0",
  "description": "NAV Foreldrepenges√∏knad",
  "scripts": {
    "build": "node src/build/scripts/production-build.js",
    "clean-build": "rimraf ./dist",
    "docker-build": "npm run clean-build && npm run build && docker build . -t foreldrepengesoknad",
    "docker-start": "docker run --name foreldrepengesoknad -p 8080:8080 -d foreldrepengesoknad",
    "docker-kill": "npm run docker-stop && npm run docker-rm",
    "docker-rm": "docker rm foreldrepengesoknad",
    "docker-stop": "docker stop foreldrepengesoknad",
    "docker-rmi": "npm run docker-kill && docker rmi foreldrepengesoknad",
    "lint": "tslint-config-prettier-check ./tslint.json",
    "start-express": "node ./server.js",
    "start-mock-api-server": "node ./mock-api-server.js",
    "start": "node ./src/build/scripts/start-dev.js",
    "precommit": "lint-staged",
    "cypress": "export PORT=8081 && npm run start-express & wait-on -v -t 15000 http://localhost:8081 && echo $(npm config get prefix)/bin/cypress run",
    "cy": "cypress open & npm run build && export PORT=8081 && npm run start-express"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [],
  "author": "NAV",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "16.3.12",
    "@types/react-dom": "16.0.5",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "css-loader": "0.28.11",
    "cypress": "2.1.0",
    "dotenv": "5.0.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "html-webpack-harddisk-plugin": "0.2.0",
    "html-webpack-plugin": "3.2.0",
    "less": "3.0.2",
    "less-loader": "4.1.0",
    "postcss-loader": "2.1.4",
    "prettier": "1.12.1",
    "style-loader": "0.21.0",
    "svg-sprite-loader": "3.7.3",
    "ts-loader": "4.2.0",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.12.0",
    "tslint-loader": "3.6.0",
    "tslint-react": "3.5.1",
    "typescript": "2.8.3",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.6.0",
    "webpack-dev-server": "3.1.3"
  },
  "dependencies": {
    "express": "4.16.3",
    "jsdom": "11.9.0",
    "mustache-express": "1.2.5",
    "promise": "8.0.1",
    "react": "16.3.2",
    "react-dom": "16.3.2"
  }
}
